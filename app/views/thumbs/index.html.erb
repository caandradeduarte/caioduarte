<h1>Listing thumbs</h1>

<table>
  <tr>
    <th>Picture</th>
    <th>Job</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @thumbs.each do |thumb| %>
  <tr>
    <td><%= thumb.picture %></td>
    <td><%= thumb.job %></td>
    <td><%= link_to 'Show', thumb %></td>
    <td><%= link_to 'Edit', edit_thumb_path(thumb) %></td>
    <td><%= link_to 'Destroy', thumb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Thumb', new_thumb_path %>
