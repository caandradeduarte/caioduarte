<h1>Listing job_descriptions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Language</th>
    <th>Job</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @job_descriptions.each do |job_description| %>
  <tr>
    <td><%= job_description.title %></td>
    <td><%= job_description.description %></td>
    <td><%= job_description.language %></td>
    <td><%= job_description.job %></td>
    <td><%= link_to 'Show', job_description %></td>
    <td><%= link_to 'Edit', edit_job_description_path(job_description) %></td>
    <td><%= link_to 'Destroy', job_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job description', new_job_description_path %>
